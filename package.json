{
  "name": "campfire-films",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "build": "turbo build",
    "clean:modules": "pnpm --recursive --parallel exec rm -rf node_modules; rm -rf node_modules;",
    "dev": "turbo dev",
    "format:fix": "prettier --write \"**/*.{ts,tsx,md}\"",
    "lint:js": "turbo lint:js",
    "lint:js:fix": "turbo lint:js:fix",
    "lint:css": "turbo lint:css",
    "lint:css:fix": "turbo lint:css:fix",
    "typecheck": "turbo typecheck"
  },
  "devDependencies": {
    "@campfire/config-eslint": "workspace:*",
    "@campfire/config-ts": "workspace:*",
    "prettier": "^3.5.1",
    "turbo": "^2.4.2",
    "typescript": "5.7.3"
  },
  "packageManager": "pnpm@10.4.1",
  "engines": {
    "pnpm": "10.4.1",
    "node": "22.11.0",
    "npm": "use pnpm",
    "yarn": "use pnpm"
  }
}
